{
  "manifest_version": 2,

  "name": "Quora sort",
  "description": "This extension will sort loaded answers based on upvotes",
  "version": "1.0",
"browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
    "matches":["https://www.quora.com/*"],
    "scripts": ["background.js"],
    "persistent": false
  },
 "permissions": [
  "activeTab"
]
}
